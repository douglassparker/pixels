<!-- 
This extension allows the maven wrapper (mvnw) to be used without 
specifically specifying the settings.xml location.

See https://github.com/takari/maven-wrapper/blob/master/README.md for 
information on the Maven wrapper.

See https://github.com/gzm55/project-settings-maven-extension/blob/master/README.md
for more information on this extension. 
-->
<extensions xmlns="http://maven.apache.org/EXTENSIONS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/EXTENSIONS/1.0.0 
		http://maven.apache.org/xsd/core-extensions-1.0.0.xsd">
	<extension>
		<groupId>com.github.gzm55.maven</groupId>
		<artifactId>project-settings-extension</artifactId>
		<version>0.1.1</version>
	</extension>
</extensions>